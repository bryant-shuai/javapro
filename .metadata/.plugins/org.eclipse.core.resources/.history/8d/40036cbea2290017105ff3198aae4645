package com.sxt.calendar;

import java.util.Scanner;

// 非递归实现斐波那契数列
public class FeiBo {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int num = input.nextInt();
        if (1 == num || 2 == num) {  
            return 1;  
        }  
        long num1 = 1;  
        long num2 = 1;  
        long temp = 0;  
        for (int i = 2; i < n; i++) {  
            temp = num2;  
            num2 += num1;  
            num1 = temp;  
        }  
        return num2; 
	}
}
